{
  "name": "tappz",
  "version": "1.0.0",
  "private": true,
  "description": "live note taking app",
  "license": "BSL",
  "main": "lib/index",
  "keywords": [
    "sentry",
    "cloud run",
    "typescript",
    "google cloud run",
    "google cloud provider"
  ],
  "scripts": {
    "start": "node ./dist/index.mjs",
    "build": "tsup",
    "watch:server": "nodemon",
    "deploy": "npm build && (npm deploy:app)",
    "deploy:app": "./bin/deploy.sh",
    "dev": "npm build --watch & NODE_ENV=development nodemon --exec \"npm start\"",
    "lint": "npm run eslint src/**/*.ts test/**/*.test.js --fix",
    "test": "npm eslint src/**/*.ts test/**/*.test.ts && npm jest",
    "test:watch": "NODE_ENV=development npm jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@automerge/automerge-repo": "^1.1.9",
    "@automerge/automerge-repo-network-websocket": "^1.1.9",
    "@automerge/automerge-repo-storage-indexeddb": "^1.1.9",
    "@automerge/automerge-repo-storage-nodefs": "^1.2.0",
    "@automerge/prosemirror": "^0.0.12",
    "@google-cloud/firestore": "^7.9.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "express": "^4.19.2",
    "firebase-admin": "^12.2.0",
    "gcp-metadata": "^6.1.0",
    "google-auth-library": "^9.11.0",
    "ioredis": "^5.4.1",
    "pino": "^9.2.0",
    "pino-http": "^10.1.0",
    "prosemirror-collab": "^1.3.1",
    "prosemirror-example-setup": "^1.2.2",
    "prosemirror-model": "^1.21.3",
    "prosemirror-schema-basic": "^1.2.2",
    "prosemirror-state": "^1.4.3",
    "prosemirror-transform": "^1.9.0",
    "prosemirror-view": "^1.21.0",
    "socket.io": "^4.7.5",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@sentry/integrations": "^5.15.5",
    "@sentry/node": "^5.12.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/lodash.isnil": "^4.0.9",
    "@types/node": "^20.12.7",
    "@types/shelljs": "^0.8.15",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": ">=4",
    "jest": "^29.7.0",
    "jest-circus": "^26.0.1",
    "jest-cli": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "lint-staged": ">=10",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.2",
    "tsup": "^8.1.0",
    "typescript": "^5.3.2",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
